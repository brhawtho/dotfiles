[user]
	name = Bryce Hawthorne
	email = hawthornebryc@gmail.com
[core]
	editor = vim
	excludesfile = /Users/brycehawthorne/.gitignore_global
[alias]
    br = branch
    ci = commit
	co = checkout
    last = log -1 HEAD
    st = status

    man = !tabs -4 && \
        tput bold && echo 'USER' && tput sgr0 && \
            tput bold && echo '  name' && tput sgr0 && \
                echo '\tBryce Hawthorne' && \
            tput bold && echo '  email' && tput sgr0 && \
                echo '\thawthornebryce@gmail.com' && \
        tput bold && echo 'ALIAS' && tput sgr0 && \
            tput bold && echo '  add - Add file contents to the index' && tput sgr0 && \
                echo '\ta\t=> git add . -> git status' && \
                echo '\taa\t=> git add . -> git add -u . -> git status' && \
                echo '\tac\t=> git add . -> git commit' && \
                echo '\tacm\t=> git add . -> git commit -m <message>' && \
                echo '\tau\t=> git add -u . -> git status' && \
            tput bold && echo '  branch - List, create, or delete branches' && tput sgr0 && \
                echo '\tbr\t=> git branch' && \
            tput bold && echo '  checkout - Switch branches or restore working tree files' && tput sgr0 && \
                echo '\tco\t=> git checkout' && \
                echo '\tmaster\t=> git checkout master' && \
            tput bold && echo '  commit - Record changes to the repository' && tput sgr0 && \
                echo '\tc\t=> git commit' && \
                echo '\tca\t=> git commit --amend' && \
                echo '\tci\t=> git commit' && \
                echo '\tcm\t=> git commit -m <message>' && \
            tput bold && echo '  diff - Show changes between commits, commit and working tree, etc' && tput sgr0 && \
                echo '\td\t=> git diff' && \
            tput bold && echo '  log - Show commit logs' && tput sgr0 && \
                echo '\tl\t=> git log --graph --all --pretty=format' && \
                echo '\tlast\t=> git log -1 HEAD' && \
                echo '\tlg\t=> git log --color --graph --pretty=format' && \
                echo '\tll\t=> git log --stat --abbrev-commit' && \
                echo '\tllg\t=> git log --color --graph --pretty=format' && \
            tput bold && echo '  status - Show the working tree status' && tput sgr0 && \
                echo '\ts\t=> git status' && \
                echo '\tst\t=> git status' && \
            tput bold && echo '  svn - Bidirectional operation between a Subversion repository and Git' && tput sgr0 && \
                echo '\tspull\t=> git svn rebase' && \
                echo '\tspush\t=> git svn commit'
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
